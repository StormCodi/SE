//Simple Essentials
public class SE {

	public static void nL() { // newLine
		System.out.println();
	}

	public static void nL(int c) {// newLine but multiple
		for (int i = 0; i < c; i++) {
			System.out.println();
		}
	}

	public static int randI(int min, int max) {// random integer
		if (min <= max) {
			int num = (int) (Math.random() * (max - min + 1)) + min;
			return num;
		}
		return -1;
	}

	public static double randD(double min, double max) {// random double
		if (min <= max) {
			double num = (double) (Math.random() * (max - min)) + min;
			return num;
		}
		return -1;
	}

	public static void print(String toPrint) {// printLn but easier
		if (toPrint != null)
			System.out.println(toPrint);
	}

	public static void print(int i) {// overloaded method
		print("" + i);

	}

	public static void print(double d) {// overloaded method
		print("" + d);
	}

	public static void printN(String toPrint) {// print easy (N = not start a new line)
		if (toPrint != null)
			System.out.print(toPrint);
	}

	public static void printN(int i) {// overloaded method
		print("" + i);

	}

	public static void printN(double d) {// overloaded method
		print("" + d);
	}

}
